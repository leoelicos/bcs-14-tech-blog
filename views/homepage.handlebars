<!-- Loop over each object in the blogs array -->
{{#each blogs as |blog|}}
  <div class='row mb-4 blog'>
    <div class='col-md-5'>
      <h2>

        <a href='/blog/{{blog.id}}'>{{blog.title}}</a>
      </h2>
<p>Category {{blog.category.category_name}}</a></p> 
 
      <p>Created by
        {{blog.user.username}}
        on
        {{blog.date}}</p>
    </div>
    <div class='col-md-7'>
      <p>
        {{blog.summary}}
      </p>
    </div>
  </div>
{{/each}}